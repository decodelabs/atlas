{
    "name": "decodelabs/atlas",
    "description": "Easy filesystem and io functions",
    "type": "library",
    "keywords": [ "filesystem", "io", "file", "directory" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/coercion": "^0.3.3",
        "decodelabs/deliverance": "^0.3.3",
        "decodelabs/enumerable": "^0.2.7",
        "decodelabs/exceptional": "^0.6.1",
        "decodelabs/monarch": "^0.1.4",
        "decodelabs/nuance": "^0.1.2",
        "decodelabs/veneer": "^0.12.8"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Atlas\\": "src/"
        },
        "files": [
            "src/Context.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.13.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
