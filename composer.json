{
    "name": "decodelabs/atlas",
    "description": "Easy filesystem and io functions",
    "type": "library",
    "keywords": [ "filesystem", "io", "file", "directory" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/coercion": "^0.3.5",
        "decodelabs/deliverance": "^0.3.4",
        "decodelabs/enumerable": "^0.2.8",
        "decodelabs/exceptional": "^0.6.3",
        "decodelabs/nuance": "^0.1.9"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Atlas\\": "src/Atlas/"
        },
        "classmap": [
            "src/Atlas.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.14.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
